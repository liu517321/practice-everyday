public ListNode deleteDuplicates2(ListNode head) {
       if(head == null || head.next == null) {
           return head;
       } 
        ListNode result = new ListNode(-1);
        result.next = head;
        ListNode pre = result;
        while(pre.next!=null && pre.next.next!=null){
            ListNode a = pre.next;
            ListNode b = pre.next.next;
            if(b.val == a.val){
                while(b != null && b.val == a.val){
                    b = b.next;
                }
                pre.next = b;
            }
            else pre = pre.next;
        }
        return result.next; 
    }
